<Window x:Class="Cabinet.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cabinet.Desktop"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="MainWindow" Height="400" Width="400">
    <Window.Resources>
        <ResourceDictionary Source="Components/Buttons.xaml"/>
    </Window.Resources>
    <Grid Background="Black" Opacity="0.9">
        <StackPanel x:Name="btnList" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="NImadir"/>
        </StackPanel>
    </Grid>
</Window>
